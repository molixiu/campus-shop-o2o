package com.xjwfk.o2o.enumes;

/**
* @ClassName: ExecutionEnum
* @Description: TODO(这个枚举类是表示增删改操作的操作成功或者失败)
* @author 白巾川
* @date 2019年7月24日
*/
public enum ExecutionEnum {
	
	SUCCESS(1, "操作成功"),FAIL(0,"操作失败");
	private int state;
	private String stateInfo;
	
	private ExecutionEnum(int state, String stateInfo) {
		this.state = state;
		this.stateInfo = stateInfo;
	}

	public int getState() {
		return state;
	}

	public String getStateInfo() {
		return stateInfo;
	}
	
	
}
